  "scripts": {
    "dev": "nx run-many --target=serve --projects=api,ui --parallel",
    "dev:ui": "nx run ui:serve",
    "dev:api": "nx run api:serve",
    "build": "nx run-many --target=build --projects=api,ui --parallel",
    "test": "nx run-many --target=test --all --parallel",
    "lint": "nx run-many --target=lint --all --parallel",
    "api:prisma:generate": "nx run api:prisma:generate",
    "api:prisma:migrate": "nx run api:prisma:migrate",
    "docker:build:ui": "docker build -f apps/ui/Dockerfile -t ui:latest .",
    "docker:build:api": "docker build -f apps/api/Dockerfile -t api:latest .",
    "docker:build:all": "docker build -f apps/ui/Dockerfile -t ui:latest . && docker build -f apps/api/Dockerfile -t api:latest .",
    "prisma:generate": "prisma generate --schema apps/api/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema apps/api/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema apps/api/prisma/schema.prisma"
  },
